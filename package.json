{
  "name": "astro-seo-enhancer",
  "version": "2.0.0",
  "description": "A powerful SEO enhancement plugin for Astro websites",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components/SEO": "./src/components/SEO.astro"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:types": "tsc",
    "build:astro": "astro build",
    "build": "npm run build:types && npm run build:astro",
    "prepare": "npm run build:types"
  },
  "keywords": [
    "astro",
    "astro-component",
    "seo",
    "meta-tags",
    "json-ld",
    "open-graph",
    "twitter-cards"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.1",
    "npm": ">=9.3.1"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/ms": "^2.1.0",
    "@vitest/coverage-v8": "^1.3.1",
    "astro": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.3.1"
  }
}
